<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="592.0" prefWidth="403.0" spacing="10.0" stylesheets="@../css/hacker_theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.signup.SignUpController">
    <children>
        <JFXTextField fx:id="name" labelFloat="true" promptText="Name">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXTextField>
        <JFXTextField fx:id="id" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Member ID">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXTextField>
        <JFXPasswordField fx:id="password" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Password">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXPasswordField>
        <JFXPasswordField fx:id="passwordcheck" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Re-Enter Password">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXPasswordField>
        <HBox>

        </HBox>
<!--        <JFXCheckBox accessibleRole="CHECK_MENU_ITEM">-->
<!--            <VBox.margin>-->
<!--                <Insets left="10.0" right="10.0" top="20.0" />-->
<!--            </VBox.margin>-->
<!--        </JFXCheckBox>-->
        <JFXTextField fx:id="mobile" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Mobile">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXTextField>
        <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="62.0" promptText="Email">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="20.0" />
            </VBox.margin>
        </JFXTextField>
        <HBox alignment="CENTER" spacing="30.0">
            <children>
                <JFXButton fx:id="saveButton" prefHeight="47.0" prefWidth="93.0" text="Save" /><!--onAction="#addMember"-->
                <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0" prefHeight="66.0" prefWidth="100.0" text="Cancel" /><!--onAction="#cancel"-->
            </children>
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
</VBox>
